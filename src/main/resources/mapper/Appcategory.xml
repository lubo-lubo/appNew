<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//org.mybatis/DTD mapper 3.0//EN"
        "http://org.mybatis/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.group.dao.AppCategoryMapper">
    <update id="modifyOne" parameterType="App_Category">
        update app_category
        set categoryName=#{categoryName},categoryCode=#{categoryCode},parentId=#{parentId}
        where id=#{id}
    </update>
</mapper>